<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.agriapp.Consultengg"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <Switch
        android:id="@+id/switch1"
        android:layout_width="239dp"
        android:layout_height="27dp"
        android:layout_marginLeft="41dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="88dp"
        android:textOff="Offline"
        android:textOn="Online"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="133dp"
        android:layout_height="33dp"
        android:layout_marginBottom="-7dp"
        android:layout_marginLeft="41dp"
        android:layout_marginStart="41dp"
        android:text="Engg1"
        app:layout_constraintBottom_toBottomOf="@+id/quickContactBadge3"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="134dp"
        android:layout_height="32dp"
        android:layout_marginBottom="64dp"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="65dp"
        android:text="Engg1"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintTop_creator="1" />

    <QuickContactBadge
        android:id="@+id/quickContactBadge1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:layout_marginTop="15dp"
        android:onClick="engg1"
        app:layout_constraintBottom_toTopOf="@+id/quickContactBadge3"
        app:layout_constraintLeft_toLeftOf="@+id/quickContactBadge3"
        app:layout_constraintRight_toRightOf="@+id/quickContactBadge3"
        app:layout_constraintTop_toBottomOf="@+id/switch1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="129dp"
        android:layout_height="33dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="41dp"
        android:layout_marginStart="41dp"
        android:text="Engg2"
        app:layout_constraintBottom_toBottomOf="@+id/quickContactBadge4"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1" />

    <QuickContactBadge
        android:id="@+id/quickContactBadge3"
        android:layout_width="93dp"
        android:layout_height="82dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:onClick="eng1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533"
        tools:layout_constraintLeft_creator="1" />

    <QuickContactBadge
        android:id="@+id/quickContactBadge4"
        android:layout_width="0dp"
        android:layout_height="83dp"
        android:layout_marginBottom="98dp"
        android:layout_marginTop="101dp"
        android:onClick="eng2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/quickContactBadge3"
        app:layout_constraintRight_toRightOf="@+id/switch1"
        app:layout_constraintTop_toTopOf="@+id/quickContactBadge3"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

</android.support.constraint.ConstraintLayout>
